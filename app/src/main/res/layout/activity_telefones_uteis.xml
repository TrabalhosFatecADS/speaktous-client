<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TelefonesUteis"
        android:background="#67b6e9">
    <TextView
            android:text="Telefones Úteis"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtConteudo" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/layoutedit" android:textSize="30dp"
            android:textStyle="bold" android:textColor="#000000" app:layout_constraintHorizontal_bias="0.0"
            android:gravity="center"/>

    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/iconex"
            android:id="@+id/btnFechar"
            android:background="#00000000" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="3dp"/>

    <TextView
            android:text="Centro de Valorização da Vida:    188"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtConteudoVida"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/layoutedit" android:textSize="24sp"
            android:textStyle="bold" android:textColor="#000000" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="40dp" app:layout_constraintTop_toBottomOf="@+id/txtConteudo"/>

    <TextView
            android:text="Assistência a Dependentes de Agentes Químicos:    132"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtConteudoQuimico"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/layoutedit" android:textSize="24sp"
            android:textStyle="bold" android:textColor="#000000" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/txtConteudoVida"/>

    <TextView
            android:text="Delegacias Especializadas de atendimento à Mulher:    180"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtConteudoDelegacia"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/layoutedit" android:textSize="24sp"
            android:textStyle="bold" android:textColor="#000000" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/txtConteudoQuimico"/>


</android.support.constraint.ConstraintLayout>